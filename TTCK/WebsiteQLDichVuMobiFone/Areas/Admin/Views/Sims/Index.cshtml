@model IEnumerable<WebsiteQLDichVuMobiFone.Models.Sim>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.SoThueBao)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.KhuVucHoaMang)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PhiHoaMang)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GoiDangKyDiKemNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IddichVuNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdloaiSoNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdtrangThaiSimNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.SoThueBao)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.KhuVucHoaMang)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PhiHoaMang)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GoiDangKyDiKemNavigation.IdgoiDangKy)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IddichVuNavigation.IddichVu)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdloaiSoNavigation.IdloaiSo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdtrangThaiSimNavigation.IdtrangThaiSim)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Idsim">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Idsim">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Idsim">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
